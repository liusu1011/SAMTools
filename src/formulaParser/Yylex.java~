        case 83: break;
        case 18: 
          { return tok(sym.MUL,null);
          }
        case 84: break;
        case 7: 
          { return tok(sym.IMP,null);
          }
        case 85: break;
        case 22: 
          { return tok(sym.EXISTS,null);
          }
        case 86: break;
        case 8: 
          { return tok(sym.EQUIV,null);
          }
        case 87: break;
        case 2: 
          { 
          }
        case 88: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            zzDoEOF();
              { {
		return tok(sym.EOF, null);
}
 }
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
